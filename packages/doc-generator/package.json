{
  "name": "@agentic-os/doc-generator",
  "version": "1.0.0",
  "description": "AI-Powered TypeScript Documentation Generator - Automatic Markdown docs from TypeScript code",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "doc-gen": "./dist/cli.js"
  },
  "exports": {
    ".": "./dist/index.js",
    "./analyzer": "./dist/analyzer/CodeAnalyzer.js",
    "./generator": "./dist/generator/TemplateEngine.js"
  },
  "files": [
    "dist",
    "templates",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run clean && npm run build",
    "doc-gen": "tsx src/cli.ts"
  },
  "keywords": [
    "typescript",
    "documentation",
    "generator",
    "ast",
    "ts-morph",
    "handlebars",
    "markdown",
    "api-docs",
    "code-analysis",
    "automation",
    "ai",
    "developer-tools"
  ],
  "author": "Agentic OS Team",
  "license": "MIT",
  "dependencies": {
    "ts-morph": "^21.0.1",
    "handlebars": "^4.7.8",
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/handlebars": "^4.1.0",
    "@vitest/coverage-v8": "^4.0.4",
    "tsx": "^4.7.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShunsukeHayashi/Autonomous-Operations.git",
    "directory": "packages/doc-generator"
  },
  "publishConfig": {
    "access": "public"
  }
}
